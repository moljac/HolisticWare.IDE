<?xml version="1.0" encoding="UTF-8" ?>
<Template>
	<TemplateConfiguration>
		<_Name>Image Assets</_Name>
		<Icon>md-xml-file-icon</Icon>
		<_Category>iOS</_Category>
		<LanguageName>*</LanguageName>
		<ProjectType>IPhone</ProjectType>
		<DefaultFilename>Images</DefaultFilename>
		<_Description>Creates an asset catalog for images.</_Description>
	</TemplateConfiguration>
	
	<Conditions>
		<ParentProject RequireProject="True"/>
		<MacProject IsAssetCatalog="True"/>
	</Conditions>
	
	<TemplateFiles>
		<RawFile name="${Name}.imageset/Contents.json" SuppressAutoOpen="False" BuildAction="ImageAsset" src="ImageSet.json" />
    </TemplateFiles>
</Template>
